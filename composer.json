{
    "name": "santanamic/php-tradingview-sdk",
    "description": "SDK for TradingView API (scraping)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "santanamic",
            "email": "williansantanamic@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "TradingView\\" : "src/TradingView" }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "scripts": {
		"lint": "php-cs-fixer fix --diff --dry-run",
        "fix": "php-cs-fixer fix"
    }
}
